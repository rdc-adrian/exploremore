{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"],
      "@/data/*": ["data/*"],
      "@/types/*": ["types/*"],
      "@/store/*": ["store/*"],
      "@/theme/*": ["theme/*"],
      "@/utils/*": ["utils/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
